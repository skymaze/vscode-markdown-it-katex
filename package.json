{
    "name": "@vscode/markdown-it-katex",
    "version": "1.0.0",
    "description": "Markdown-it plugin that provides VS Code's KaTeX support",
    "main": "dist/index.js",
    "files": [
        "dist/index.js"
    ],
    "scripts": {
        "compile": "tsc -p .",
        "test": "node test/all.js",
        "test-render": "node test/test-render.js test/test.md > test/out.html",
        "postinstall": "tsc --outDir ./build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/vscode-markdown-it-katex.git"
    },
    "keywords": [
        "markdown",
        "KaTeX",
        "math",
        "LaTeX",
        "markdown-it-plugin",
        "markdown-it"
    ],
    "author": {
        "name": "Microsoft Corporation"
    },
    "license": "MIT",
    "dependencies": {
        "katex": "^0.16.4"
    },
    "devDependencies": {
        "@types/katex": "^0.16.0",
        "@types/markdown-it": "^12.2.3",
        "markdown-it": "^11.0.0",
        "markdown-it-testgen": "^0.1.6",
        "tape": "^5.6.0",
        "typescript": "^4.9.5"
    }
}
